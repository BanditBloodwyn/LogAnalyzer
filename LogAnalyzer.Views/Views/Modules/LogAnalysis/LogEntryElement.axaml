<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:logAnalysis="clr-namespace:LogAnalyzer.Models.Modules.LogAnalysis;assembly=LogAnalyzer.Models"
             mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="450"
             x:Class="LogAnalyzer.Views.Views.Modules.LogAnalysis.LogEntryElement"
             x:DataType="logAnalysis:LogEntryModel">


    <Border BorderBrush="Black" BorderThickness="1">
        <Grid ColumnDefinitions="150, *">
            <Label Grid.Column="0" Content="{Binding TimeStamp}"/>
            <Label Grid.Column="1" Content="{Binding Message}"/>
        </Grid>
	</Border>
</UserControl>
